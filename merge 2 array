#include <stdio.h>
int main()
{
    int arr1[] = {1, 2, 3, 4, 5};
    int arr2[] = {6, 7, 8, 9, 10};
    int arr_result[11];
    for (int i = 0; i < 5; i++)
    {
        arr_result[i] = arr1[i];
    }
    for (int i = 5, j = 0; i < 10 && j < 6; i++, j++)
    {
        arr_result[i] = arr2[j];
    }
    for (int i = 0; i < 11; i++) 
    {
        printf("%d ", arr_result[i]);
    }
    return 0;
}
