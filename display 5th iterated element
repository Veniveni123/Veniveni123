#include <stdio.h>
int main()
{
    int n;
    printf("Enter number of elements in the array: ");
    scanf("%d", &n);
    int arr[n];
    printf("Enter %d elements in the array: ",n);
    for(int i=0;i<n;i++)
    {
        scanf("%d", &arr[i]);
    }
    printf("The 5th iterated element in the array is: %d", arr[4]);
    return 0;
}
